<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ZAVIT</title>
    @RenderSection("meta", false)
    <!-- Google Tag Manager -->
