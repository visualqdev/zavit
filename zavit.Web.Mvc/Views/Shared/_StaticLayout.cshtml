                        <a class="navbar-brand hidden-xs" href="/">zavit</a>
                        <a class="navbar-brand visible-xs" href="/">z</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="row content">
            @RenderBody()
        </div>
        <div class="row footer">
            <footer>&copy; zavit 2017 | <a href="@Url.Action("Index", "TermsAndConditions")">Terms & Conditions</a></footer>
        </div>
    </div>
<script src="/libraries/jquery-3.1.0.min.js"></script>
<script src="/CSS/bootstrap/js/bootstrap.min.js"></script>
